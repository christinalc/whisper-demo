{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "require-corp"
          },
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin"
          }
        ]
      },
      {
        "source": "/node_modules/@ffmpeg/core/dist/ffmpeg-core.wasm",
        "headers": [
        {
            "key": "Content-Type",
            "value": "application/wasm"
        }
      ]
      }
    ]
  }
  
  